<Window x:Class="Admin.AdminGUI"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Admin"
        mc:Ignorable="d"
        Title="Admin" Height="450" Width="800">
    <Grid>

        <Image x:Name="logoGeneral" HorizontalAlignment="Left" Height="64" Margin="10,10,0,0" VerticalAlignment="Top" Width="119" Source="/clipart16522.png"/>
        <TextBox x:Name="TextBoxStaff_Id" IsReadOnly="True" 
                 HorizontalAlignment="Left" Margin="309,130,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="171" Background="#FFABADB3"/>
        <TextBox x:Name="TextBoxStaff_Name"  
                 HorizontalAlignment="Left" Margin="309,192,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="171"/>
        <Label x:Name="LabelStaff_Id" Content="Staff ID" HorizontalAlignment="Left" Margin="309,99,0,0" VerticalAlignment="Top"/>
        <Label x:Name="LabelStaff_Name" Content="Staff Name" HorizontalAlignment="Left" Margin="309,161,0,0" VerticalAlignment="Top"/>
        <Label x:Name="LabelCommand" Content="" HorizontalAlignment="Center" Height="157" Margin="0,217,0,0" VerticalAlignment="Top" Width="303"/>
        <StatusBar Margin="0,413,359,0"/>

    </Grid>
</Window>
